{
  "name": "suilven/template-env-checker",
  "type": "silverstripe-vendormodule",
  "description": "Adds ability to check environment from within a template.",
  "homepage": "https://github.com/suilven/template-env-checker",
  "keywords": ["SilverStripe","template","environment"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gordon Anderson",
      "role": "Developer",
      "email": "gordon.b.anderson@gmail.com",
      "homepage": "https://github.com/gordonbanderson"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverstripe/framework": "^4@dev"
  },
  "autoload": {
    "psr-4": {
      "Suilven\\Env\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
